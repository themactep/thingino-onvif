{
    "model": "Model",
    "manufacturer": "Manufacturer",
    "firmware_ver": "0.0.1",
    "serial_num": "SN1234567890",
    "hardware_id": "HWID",
    "ifs": "wlan0",
    "port": 80,
    "username": "thingino",
    "password": "thingino",
    "scopes": [
        "onvif://www.onvif.org/Profile/Streaming",
        "onvif://www.onvif.org/Profile/T",
        "onvif://www.onvif.org/hardware",
        "onvif://www.onvif.org/name"
    ],
    "profiles": {
        "stream0": {
            "name": "Profile_0",
            "width": 1920,
            "height": 1080,
            "url": "rtsp://%s/ch0",
            "snapurl": "http://%s/image.jpg",
            "type": "H264",
            "audio_encoder": "AAC",
            "audio_decoder": "AAC"
        },
        "stream1": {
            "name": "Profile_1",
            "width": 640,
            "height": 360,
            "url": "rtsp://%s/ch1",
            "snapurl": "http://%s/image.jpg",
            "type": "H264",
            "audio_encoder": "AAC",
            "audio_decoder": "AAC"
        }
    },
    "ptz": {
        "enable": 0,
        "min_step_x": 0,
        "max_step_x": 4000,
        "min_step_y": 0,
        "max_step_y": 2000,
        "min_step_z": 0,
        "max_step_z": 0,
        "get_position": "/bin/motors -p",
        "is_moving": "/bin/motors -b",
        "move_left": "/bin/motors -d h -x 0",
        "move_right": "/bin/motors -d h -x 4000",
        "move_up": "/bin/motors -d h -y 0",
        "move_down": "/bin/motors -d h -y 2000",
        "move_in": "/sbin/ptz_zoom_in",
        "move_out": "/sbin/ptz_zoom_out",
        "move_stop": "/bin/motors -d s",
        "move_preset": "/sbin/ptz_presets %d > /dev/null",
        "goto_home_position": "/bin/motors -d b",
        "set_preset": "/sbin/ptz_presets -a %d %s > /dev/null",
        "set_home_position": "/sbin/ptz_set_home",
        "remove_preset": "/sbin/ptz_presets -r %d > /dev/null",
        "jump_to_abs": "/bin/motors -d h -x %f -y %f",
        "jump_to_rel": "/bin/motors -d g -x %f -y %f",
        "get_presets": "/sbin/ptz_presets -g",
        "continuous_move": "/bin/motors -d h -x %f -y %f"
    },
    "relays": [
        {
            "idle_state": "close",
            "close": "ircut off",
            "open": "ircut on"
        },
        {
            "idle_state": "close",
            "close": "irled off",
            "open": "irled on"
        }
    ],
    "events_enable": 1,
    "events": [
        {
            "topic": "tns1:VideoSource/MotionAlarm",
            "source_name": "VideoSourceConfigurationToken",
            "source_type": "tt:ReferenceToken",
            "source_value": "VideoSourceConfigToken",
            "input_file": "/run/motion/motion_alarm"
        },
        {
            "topic": "tns1:Device/HardwareFailure",
            "source_name": "Source",
            "source_type": "tt:ReferenceToken",
            "source_value": "HardwareId",
            "input_file": "/tmp/hardware_failure"
        }
    ],
    "aux": [
        { "name": "IRLED ON",  "exec": "irled on"  },
        { "name": "IRLED OFF", "exec": "irled off" },
        { "name": "COLOR ON",  "exec": "color on"  },
        { "name": "COLOR OFF", "exec": "color off" }
    ],
    "adv_enable_media2": false,
    "adv_fault_if_set": false,
    "adv_fault_if_unknown": false,
    "adv_synology_nvr": false,
    "loglevel": "ERROR",
    "raw_log_directory": "",
    "raw_log_on_error_only": false
}
