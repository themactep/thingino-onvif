<?xml version="1.0" encoding="UTF-8"?>
<SOAP-ENV:Envelope xmlns:SOAP-ENV="http://www.w3.org/2003/05/soap-envelope"
                   xmlns:tr2="http://www.onvif.org/ver20/media/wsdl"
                   xmlns:tt="http://www.onvif.org/ver10/schema">
    <SOAP-ENV:Body>
        <tr2:GetAudioOutputConfigurationsResponse>
            <tr2:Configurations token="%AUDIO_OUTPUT_CONFIG_TOKEN%">
                <tt:Name>%AUDIO_OUTPUT_NAME%</tt:Name>
                <tt:UseCount>%PROFILES_NUM%</tt:UseCount>
                <tt:OutputToken>%AUDIO_OUTPUT_TOKEN%</tt:OutputToken>
                <tt:SendPrimacy>www.onvif.org/ver20/HalfDuplex/Client</tt:SendPrimacy>
                <tt:OutputLevel>%AUDIO_OUTPUT_LEVEL%</tt:OutputLevel>
            </tr2:Configurations>
        </tr2:GetAudioOutputConfigurationsResponse>
    </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
